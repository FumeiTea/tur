#?:app:zh
#- -l | --list | list  显示全部类别
#- -ll | --list-all | list-all  显示全部类别及values
#?:app:zh
app(){
    [[ $@ ]] || set -- -ll
    case $1 in
        -l | --list | list )
            #eval echo "$(printf "\${%s[@]}\n" "${APP[@]}")"
            [[ $2 ]] && eval echo "$2 : \${$2[@]}" || echo ${APP[@]}
                ;;
        -ll | --list-all | list-all )
                for p in ${APP[@]}; do
                    eval echo "$p : \${$p[@]}"
                done
            ;;
        * )
            [[ "${APP[@]}" =~ $1 ]] && {
                echo $1
            }
            ;;
    esac
}


