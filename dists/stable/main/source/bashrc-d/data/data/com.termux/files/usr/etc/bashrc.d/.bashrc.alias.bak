        --install-existing | in-ex )
        --disable | dis )
        --enable | en | e )
        --package-name | cat )
        --package-name-all | cat-all )
        --info | info )
        --ext | ext )
        --app-name | --name | appname | name | n )
        --battery-whitelist-add | bw+ )
