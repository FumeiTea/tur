## inner link & URL Scheme & deep link ##
am_nd(){
    local n d a user other
    #[[ $@ =~ " -- " ]] && other=$(echo $@ |sed "s/.* -- //")
    [[ $1 == - ]] && local n='' || {
        a=$(echo $1 |cut -d / -f 3)
        n=$(echo $1 |cut -d / -f 1,2)
        user=$(echo $1 |cut -d / -f 4) 
        [[ $a ]] && a="-a $a"
        [[ $n =~ ^/.*$ ]] && n='' || n="-n $n"
        [[ $user =~ ^[0-9]+$ ]] && user="--user $user" || user="--user 0"
    }
    [[ $2 ]] && d="-d $2"
    shift 2
    am_start $user $a $n $d $@ #&>/dev/null
}

