        - )
        * | --help | -h )
@
#?:am_start:zh

#- 

#- rishc dumpsys deviceidle whitelist +com.google.android.deskclock
am_start(){
    local n user _user package_name package_name_all action _action appname ttype _ttype
    unset n user _user package_name package_name_all action _action appname ttype _ttype

    [[ "$1" && "$1" != - ]] && {
        package_name=$(echo $1 |cut -d / -f 1)
        #class_name=$(echo $1 |cut -d / -f 2)
        package_name_all=$(echo $1 |cut -d / -f 1,2)
        action=$(echo "$1" |cut -d / -f 3)
        appname=$(echo "$1" |cut -d / -f 5)
        ttype=$(echo "$1" |cut -d / -f 6,7)
    }
    [[ "$ttype" =~ ^[a-z*]+/[a-z*]+$ ]] && {
        _ttype[1]="-t"
        _ttype[2]="$ttype"
    }
    [[ "$2" =~ ^[0-9]+$ ]] && {
        user=$2
        _user="--user $user"
        set -- "$1" - "${@:3}"
    } || user=$(echo $1 |cut -d / -f 4)
    [[ $user =~ ^[0-9]+$ ]] && _user="--user $user" || {
        user=0
        _user="--user 0"
    }
    [[ $3 =~ ^[0-9]+$ ]] && _user="--user $3" && set -- "${@:1:2}" "${@:4}"

    [[ $action ]] && _action="-a $action"

    [[ $package_name ]] && {
        [[ ${package_name_all: -1} == '/' ]] && n="$package_name" || n="-n $package_name_all"
    }

    [[ $2 ]] || set -- "$1" -
    case "$2" in
        --install | in )
            [[ $user == 0 ]] && \
            #am start -a android.intent.action.VIEW -d "market://search?q=pname:${1%/*}" 2>/dev/null
                am start $_user -a android.intent.action.VIEW -d "market://details?id=$package_name" 2>/dev/null || \
                rish -c "am start $_user -a android.intent.action.VIEW -d \"market://details?id=$package_name\" " 2>/dev/null
            return
            ;;
        --uninstall | unin | uni )
            rish -c "pm uninstall $_user $package_name"
            return
            ;;
        --uninstall-j | unin-k | uni-k | unik | uk )
            rish -c "pm uninstall $_user $package_name"
            return
            ;;
        --install-existing | in-ex )
            rish -c "pm install-existing $_user $package_name"
            return
            ;;
        --disable | dis )
            rish -c "pm disable-user $_user $package_name"
            return
            ;;
        --enable | en | e )
            rish -c "pm enable $_user $package_name"
            return
            ;;
        --package-name | cat )
            echo $package_name
            return
            ;;
        --package-name-all | cat-all )
            echo $package_name_all
            return
            ;;
        --info | info )
            [[ $user == 0 ]] && \
                am start -a android.settings.APPLICATION_DETAILS_SETTINGS -n com.android.settings/com.android.settings.applications.InstalledAppDetails -d package:$package_name $_user &>/dev/null || \
                rish -c "am start -a android.settings.APPLICATION_DETAILS_SETTINGS -n com.android.settings/com.android.settings.applications.InstalledAppDetails -d package:$package_name $_user" &>/dev/null
            return
            ;;
        --ext | ext )
            [[ $user == 0 ]] && \
                am start -a android.intent.action.VIEW $_action $_user $3 &>/dev/null || \
                rish -c "am start -a android.intent.action.VIEW $_action $_user $3" &>/dev/null
            return
            ;;
        --app-name | --name | appname | name | n )
            [[ $3 =~ ^[a-z]{2}-[A-Z]{2}$ ]] && lang=$3 || \
            lang=$(getprop persist.sys.locale)
            appname_tmp=$(echo $appname |sed -n "s/.*${lang}:\([^:]\+\).*/\1/p" |tr -d ' \t\n\r')
            [[ ${appname_tmp} ]] && echo ${appname_tmp} || echo $appname
            return
            ;;
        --battery-whitelist-add | bw+ )
            battery_whitelist + $package_name
            return
            ;;
        --battery-whitelist-remove | bw- )
            battery_whitelist - $package_name
            return
            ;;
        - )
            echo - am start $_user $n $_action
            [[ $user == 0 ]] && \
                am start $_user $n $_action &> /dev/null || \
                rish -c "am start $_user $n $_action" &> /dev/null
            return
            ;;
        * | --help | -h )

            [[ $2 && ! $2 =~ ^- ]] && d="-d $2" || d="$2"
            echo am start $_user $_action "${_ttype[@]}" $d ${@:3} $n #&>/dev/null
            am start $_user $_action "${_ttype[@]}" $d ${@:3} $n &>/dev/null
            return
            ;;
    esac

}

