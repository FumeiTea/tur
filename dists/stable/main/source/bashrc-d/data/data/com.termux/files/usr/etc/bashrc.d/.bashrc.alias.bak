file_manage(){

    [[ $# == 0 ]] && return 9
    echo $@
    
    local a mime_type
    declare -a a[{0..6}]=
    a=($(echo $1 |tr '/' ' ') "${a[@]}") && shift
    [[ $1 && $1 =~ ^[0-9]+$ ]] && a[3]=$1 && shift
    
    mime_type=$(file -b --mime-type $1 2>/dev/null)
    [[ $mime_type =~ ' ' ]] || a[5]="$mime_type"
    case $mime_type in
        inode/directory )
            a[1]=
            a[2]=android.intent.action.OPEN_DOCUMENT
            a[5]='*/*'
            ;;
        image/* )
            a[1]=
            a[2]=android.intent.action.VIEW
            set -- "file://$(realpath $1)" "${@:2}"
            ;;
        text/* )
            a[1]=
            a[2]=android.intent.action.EDIT
            set -- "file://$(realpath $1)" "${@:2}"
            ;;
        * )
            ;;
    esac
    a=$(echo "${a[@]}" |tr ' ' '/')
    echo 1 am_start $a $@ 
    am_start $a $@ 

}

